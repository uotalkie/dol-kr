	<</if>>
	<<destination_catacombs skip>>
<</addinlineevent>>
<<addinlineevent "catacombs_7" 1>>
	<<if $worn.feet.type.includes("naked")>>	
		<<if $catacombs_torch gte random(1, 100)>>
