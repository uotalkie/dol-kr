<</widget>>

<<widget "presetConfirmDetails">>
<<set _presetObject to JSON.parse(_preset)>>
<span class="green">아래와 같이 설정을 변경하겠습니까?</span> |
<<link "네">><<run importSettings(_preset, "function")>><<displaySettings "quickStart">><<if StartConfig.enableImages is true and $passage is "Start">><<startingPlayerImageUpdate>><<removeclass #startingPlayerImage "hidden">><</if>><</link>> |
<<link "아니오">><<displaySettings "quickStart">><</link>>
<br><br>
<<importDetailsDisplay _presetObject>>
<</widget>>

<<widget "importConfirmDetails">>
<<if isJsonString($importString)>>
	<<link "네">><<run importSettings($importString, "function")>><<displaySettings "exportsettings">><<if StartConfig.enableImages is true and $passage is "Start">><<startingPlayerImageUpdate>><<startingPlayerImageUpdate>><</if>><</link>> |
	<<link "아니오">><<displaySettings "exportsettings">><</link>>
	<br><br>
	<<importDetailsDisplay `JSON.parse($importString)`>>
<<else>>
	<<link "뒤로">><<displaySettings "exportsettings">><</link>>
	<br><br>
	<span class="red">Import is invalid</span>. Please double check the settings import string before importing again.
<</if>>
<</widget>>

<<widget "importDetailsDisplay">>
<<if _args[0]>>
	<div class="presetConfirm">
		<<if _args[0].starting isnot undefined>>
			<span class="gold">"starting":{</span>
			<<set _validatorObject to settingsObjects("starting")>>
			<<presetConfirm _args[0].starting>>
			<<if _args[0].starting.player isnot undefined>>
				"player":
				<<set _validatorObject to settingsObjects("starting")>>
				{<<presetConfirm _args[0].starting.player>>}
			<</if>>
			<<if _args[0].starting.skinColor isnot undefined>>
				"skinColor":
				<<set _validatorObject to settingsObjects("starting")>>
				{<<presetConfirm _args[0].starting.skinColor>>}
			<</if>>
			<span class="gold">}</span>
			<br>
		<</if>>

		<<if _args[0].general isnot undefined>>
			<span class="gold">"general":{</span>
			<<set _validatorObject to settingsObjects("general")>>
			<<presetConfirm _args[0].general>>
			<<if _args[0].general.skinColor isnot undefined>>
				"skinColour":
				<<set _validatorObject to settingsObjects("general")>>
				{<<presetConfirm _args[0].general.skinColor>>}
			<</if>>
			<<if _args[0].general.map isnot undefined>>
				"map":
				<<set _validatorObject to settingsObjects("general")>>
				{<<presetConfirm _args[0].general.map>>}
			<</if>>
			<<if _args[0].general.shopDefaults isnot undefined>>
				"shopDefaults":
				<<set _validatorObject to settingsObjects("general")>>
				{<<presetConfirm _args[0].general.shopDefaults>>}
			<</if>>
			<<if _args[0].general.options isnot undefined>>
				"options":
				<<set _validatorObject to settingsObjects("general")>>
				{<<presetConfirm _args[0].general.options>>}
			<</if>>
			<span class="gold">}</span>
			<br>
		<</if>>

		<<if _args[0].npc isnot undefined>>
			<span class="gold">"npc":{</span>
			<<set _validatorObject to settingsObjects("npc")>>
			<br>
			<<for $_label, $_value range _args[0].npc>>
				"<<print $_label>>":
				{<<presetConfirm $_value $_label>>}
			<</for>>
			<span class="gold">}</span>
		<</if>>
	</div>
<</if>>
<</widget>>

<<widget "presetConfirm">>
<<if _args[0]>>
	<<if _args[0].breastsizemin gt _args[0].breastsizemax>>
		<<set _temp to _args[0].breastsizemin>>
		<<set _args[0].breastsizemin to _args[0].breastsizemax>>
		<<set _args[0].breastsizemax to _temp>>
	<</if>>
	<<if _args[0].penissizemin gt _args[0].penissizemax>>
		<<set _temp to _args[0].penissizemin>>
		<<set _args[0].penissizemin to _args[0].penissizemax>>
		<<set _args[0].penissizemax to _temp>>
	<</if>>
	<ul>
	<<for $_label, $_value range _args[0]>>
		<<unset _altValue>>
		<<unset _altValue2>>
		<<if ["player","skinColor","map","shopDefaults","options"].includes($_label)>>
			<<continue>>
		<</if>>
		<<unset _object>>
		<<unset _valid>>
		<<set _object to _validatorObject[$_label]>>
		<<if _object isnot undefined>>
			<<set _valid to validateValue(_object, $_value)>>
			<<if Object.keys(_object).includes("boolLetter")>>
				<<set _oldValue to V[$_label]>>
				<<if _oldValue is "t">>
					<<set _altValue to "true">>
				<<elseif _oldValue is "f">>
					<<set _altValue to "false">>
				<</if>>
				<<if $_value is "t">>
					<<set _altValue2 to "true">>
				<<elseif $_value is "f">>
					<<set _altValue2 to "false">>
				<</if>>
			<</if>>
		<</if>>
		<li>
			<<switch $_label>>
				<<case "numberify_enabled">>
					<<print $_label>> 설정을 <span class="green"><<print (_altValue isnot undefined ? _altValue :`$`+$_label)>></span>에서 <span class="green"><<print (_altValue2 isnot undefined ? _altValue2 : $_value)>></span>으로 변경 (숫자 링크 탐색 활성화)
				<<case "dgchance">>
					<<print $_label>> 설정을 <span class="green"><<print (_altValue isnot undefined ? _altValue :`$`+$_label)>></span>에서 <span class="green"><<print (_altValue2 isnot undefined ? _altValue2 : $_value)>></span>으로 변경 (자지가 달린 여성 비율)
				<<case "cbchance">>
					<<print $_label>> 설정을 <span class="green"><<print (_altValue isnot undefined ? _altValue :`$`+$_label)>></span>에서 <span class="green"><<print (_altValue2 isnot undefined ? _altValue2 : $_value)>></span>으로 변경 (보지가 달린 남성 비율)
				<<case "timestyle">>
					<<print $_label>> 설정을 <span class="green"><<print $timestyle>></span>에서 <span class="green"><<print (_altValue2 isnot undefined ? _altValue2 : $_value)>></span>으로 변경 (military는 24시간제, ampm은 12시간제)
				<<case "alwaysBackToShopButton" "color" "colourItems" "compactMode" "disableReturn" "highContrast" "mannequinGender" "mannequinGenderFromClothes" "noHelp" "noTraits" "secColor">>
					<<set _object to _validatorObject["shopDefaults"][$_label]>>
					<<if _object isnot undefined>>
						<<set _valid to validateValue(_object, $_value)>>
					<</if>>
					<<print $_label>> 설정을 <span class="green"><<print `$shopDefaults.`+$_label>></span>에서 <span class="green"><<print (_altValue2 isnot undefined ? _altValue2 : $_value)>></span>으로 변경
				<<case "neverNudeMenus" "showCaptionText" "sidebarStats" "sidebarTime" "combatControls" "mapMovement" "mapTop" "mapMarkers" "images" "combatImages" "bodywritingImages" "silhouetteEnabled" "tanImgEnabled" "tanningEnabled" "images" "sidebarAnimations" "blinkingEnabled" "combatAnimations" "halfClosedEnabled" "characterLightEnabled" "lightSpotlight" "lightGradient" "lightGlow" "lightFlat" "lightCombat" "lightTFColor" "maxStates" "newWardrobeStyle" "useNarrowMarket" "skipStatisticsConfirmation" "playtime">>
					<<set _object to _validatorObject["options"][$_label]>>
					<<if _object isnot undefined>>
						<<set _valid to validateValue(_object, $_value)>>
					<</if>>
					Changes <<print $_label>> from <span class="green"><<print `$options.`+$_label>></span> to <span class="green"><<print (_altValue2 isnot undefined ? _altValue2 : $_value)>></span>
				<<case "natural" "range" "tanImgEnabled" "tanningEnabled">>
					<<set _object to _validatorObject["skinColor"][$_label]>>
					<<if _object isnot undefined>>
						<<set _valid to validateValue(_object, $_value)>>
					<</if>>
					<<print $_label>> 설정을 <span class="green"><<print `$skinColor.`+$_label>></span>에서 <span class="green"><<print (_altValue2 isnot undefined ? _altValue2 : $_value)>></span>으로 변경
				<<case "movement" "top" "markers">>
					/*<<set _object to _validatorObject["options"]["map" + $_label.toUpperFirst()]>>*/
					<<set _valid to false>>
					<<if _object isnot undefined>>
						<<set _valid to validateValue(_object, $_value)>>
					<</if>>
					<<print $_label>> 설정을 <span class="green"><<print `$map.`+$_label>></span>에서 <span class="green"><<print (_altValue2 isnot undefined ? _altValue2 : $_value)>></span>으로 변경
				<<case "pronoun" "gender" "penissize" "breastsize" "bottomsize">>
					<<if _args[1]>>
						<<print $_label>> 설정을
						<span class="green"><<print $NPCName[$NPCNameList.indexOf(_args[1])][$_label]>></span>에서
						<span class="green"><<print (_altValue2 isnot undefined ? _altValue2 : $_value)>></span>으로 변경
						<<if $_value is "none" and $passage is "Start">>
							<<set _valid to true>>
						<</if>>
					<<elseif $_label is "gender" or $_label is "breastsize" or $_label is "penissize" or $_label is "bottomsize">>
						<<set _object to _validatorObject["player"][$_label]>>
						<<if _object isnot undefined>>
							<<set _valid to validateValue(_object, $_value)>>
						<</if>>
						<<print $_label>> 설정을 <span class="green"><<print `$player.`+$_label>></span>에서 <span class="green"><<print (_altValue2 isnot undefined ? _altValue2 : $_value)>></span>으로 변경
					<<else>>
						<<print $_label>> 설정을 <span class="green"><<print (_altValue isnot undefined ? _altValue :`$`+$_label)>></span>에서 <span class="green"><<print (_altValue2 isnot undefined ? _altValue2 : $_value)>></span>으로 변경
					<</if>>
				<<case "gender_body">>
					<<set _object to _validatorObject["player"][$_label]>>
					<<if _object isnot undefined>>
						<<set _valid to validateValue(_object, $_value)>>
					<</if>>
					<<print $_label>> 설정을 <span class="green"><<print `$player.`+$_label>></span>에서 <span class="green"><<print (_altValue2 isnot undefined ? _altValue2 : $_value)>></span>으로 변경 (체형)
				<<case "ballsExist">>
					<<set _object to _validatorObject["player"][$_label]>>
					<<if _object isnot undefined>>
						<<set _valid to validateValue(_object, $_value)>>
					<</if>>
					<<print $_label>> 설정을 <span class="green"><<print `$player.`+$_label>></span>에서 <span class="green"><<print (_altValue2 isnot undefined ? _altValue2 : $_value)>></span>으로 변경 (후타나리 고환)
				<<case "freckles">>
					<<set _object to _validatorObject["player"][$_label]>>
					<<if _object isnot undefined>>
						<<set _valid to validateValue(_object, $_value)>>
					<</if>>
					<<print $_label>> 설정을 <span class="green"><<print `$player.`+$_label>></span>에서 <span class="green"><<print (_altValue2 isnot undefined ? _altValue2 : $_value)>></span>으로 변경
				<<default>>
					<<print $_label>> 설정을 <span class="green"><<print (_altValue isnot undefined ? _altValue :`$`+$_label)>></span>에서 <span class="green"><<print (_altValue2 isnot undefined ? _altValue2 : $_value)>></span>으로 변경
			<</switch>>
			<<if _valid is undefined>>
				<span class="red">(설정값이 인식되지 않아 가져올 수 없습니다)</span>
			<<elseif _valid is false>>
				<span class="red">(설정값이 유효하지 않아 가져올 수 없습니다)</span>
			<</if>>
		</li>
	<</for>>
	</ul>
<</if>>
<</widget>>
