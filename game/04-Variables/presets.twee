			<<set _preset to '{"general":{"alluremod":1,"cheatdisabletoggle":"f","checkstyle":"words","clothesPrice":2,"multipleWardrobes":"isolated"},"starting":{"gamemode":"hard"}}'>>
		<<case "gameMasochistCheats">>
