	<<set $_hpToReduce to _args[1] || $NPCList[_args[0]].healthmax>>
	<<set $_arousalToReduce to _args[2] || $enemyarousalmax/$enemyno>>
	<<if _args[0] isnot undefined and $NPCList[_args[0]]>>
