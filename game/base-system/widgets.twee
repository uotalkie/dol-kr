<<widget "controlloss">>
<<if $gamemode is "soft">>
<<elseif $consensual is 0>>
	<<set $pullaway to 0>>
	<<set $novaginal to 0>>
	<<set $noanal to 0>>
	<<set $nopenile to 0>>
