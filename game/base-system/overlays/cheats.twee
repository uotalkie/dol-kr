<</widget>>

<<widget "cheats-characterStats">>
	<div class="settingsGrid">
		<div class="settingsHeader options">플레이어</div>
		<div class="settingsToggleItemWide">
			<span class="gold">기타</span>
			<br>
			<<numberStepper "소지금" $money "money" {
				activeButtons: ["single", "double", "triple"],
				step: 1000,
				max: 100000000,
				colorArr:["--yellow"],
				percentage: false,
				valueFormat: (value) => `<b>${formatMoney(value)}</b>`
			}>>

			<<if $location is "prison">>
				<<set $prison.teeth to $prison.teeth or 0>>
				<<numberStepper "이빨" $prison.teeth {callback: (value) => V.prison.teeth = value, disableMinMax: true, step: 1, max: 1000000, colorArr:["--yellow"], percentage: false}>>
			<</if>>

			무한 스프레이: <span id="statsinfinitespray"><<= ($infinitespray ? "사용" : "미사용")>></span> |
			<<link "변경">>
				<<if $infinitespray is 1>>
					<<set $infinitespray to 0>>
				<<else>>
					<<spray 5>>
					<<set $infinitespray to 1>>
				<</if>>
				<<replace "#statsinfinitespray">><<= ($infinitespray ? "On" : "Off")>><</replace>>
			<</link>>
			<br>

			<<if $debug is 1>>
				페로몬 (DEBUG): <span id="statsalluretest"><<= $alluretest ? "사용" : "미사용">></span> |
				<<link "변경">>
					<<if $alluretest is 1>>
						<<set $alluretest to 0>>
					<<else>>
						<<set $alluretest to 1>>
					<</if>>
					<<replace "#statsalluretest">><<= $alluretest>><</replace>>
				<</link>>
				<br>
			<</if>>
		</div>
		<div class="settingsToggleItem">
			<span class="gold">신체</span> <mouse class="tooltip linkBlue">(?)<span>플레이어의 생식기를 변경하면, 존재하지 않은 신체 부위에 음란한 액체가 남아있는 것을 방지하기 위해 모든 액체가 씻겨질 것입니다.</span></mouse>
			<br>
			성별:
			<span id="statsGender">
				<<cheatsGender>>
			</span>
			<br>
			<<set _gender_body_words to {"m": "남성적", "f": "여성적", "a": "중성적"}>>
			체형:
			<span id="statsBodyType">
				<<cheatsBody>>
			</span>
			<br>
			<<set _bodysize_words to ["조그마함","작음","평범함","큼"]>>
			신체 크기:
			<span id="statsBodySize">
				<<cheatsBodySize>>
			</span>
		</div>
		<div class="settingsToggleItem">
			<span class="gold">순결</span>
			<br>
			<<set _virginity to clone($player.virginity)>>
			<<if $player.vaginaExist or $player.virginity.vaginal isnot true>>
				보지: <<cheatVirginityToggle "vaginal">>
				<br>
			<</if>>
			<<if $player.penisExist or $player.virginity.penile isnot true>>
				자지: <<cheatVirginityToggle "penile">>
				<br>
			<</if>>
			항문: <<cheatVirginityToggle "anal">>
			<br>
			입: <<cheatVirginityToggle "oral">>
			<br>
			손: <<cheatVirginityToggle "handholding">>
			<br>
			첫 키스: <<cheatVirginityToggle "kiss">>
		</div>
		<div class="settingsToggleItem">
			<span class="gold">상태</span>
			<br>
			<<set _state to ["pain", "arousal", "tiredness", "stress", "trauma", "drunk", "drugged", "hallucinogen"]>>

			<<link "상쾌함">>
				<<set $control = 1000>><<clamp>>
				<<for _i range _state>>
					<<set V[_i] to 0>><<clamp>>
				<</for>>
				<<replace "#cheatstate">><<cheatSection "state">><</replace>>
				<<updatesidebarimg>>
			<</link>> |

			<<link "망가짐">>
				<<set $control = 0>><<clamp>>
				<<for _i range _state>>
					<<set V[_i] to 20000>><<clamp>>
				<</for>>
				<<replace "#cheatstate">><<cheatSection "state">><</replace>>
				<<updatesidebarimg>>
			<</link>>
			<br><br>
			<div id="cheatstate">
				<<cheatSection "state">>
			</div>
		</div>
		<div class="settingsToggleItem">
			<span class="gold">체액:</span>
			<<link "재충전시킨다">>
				<<set $semen_amount to $semen_volume>>
				<<set $milk_amount to $milk_volume>>
				<<replace "#fluids">><<cheats-fluids>><</replace>>
			<</link>> | <<link "고갈시킨다">>
				<<set $semen_amount to 0>>
				<<set $milk_amount to 0>>
				<<replace "#fluids">><<cheats-fluids>><</replace>>
			<</link>>
			<br>
			<span class="gold">젖 분비:</span> <span id="statslactatingspray"><<= ($lactating is 1 ? "네" : "아니오")>></span> |	<<link "변경">><<set $lactating to ($lactating is 1 ? 0 : 1)>><<set $lactation_pressure to $lactating * 100>><<replace "#statslactatingspray">><<= ($lactating is 1 ? "네" : "아니오")>><</replace>><<updatesidebarimg>><</link>>
			<br><br>
			<span id="fluids"><<cheats-fluids>></span>
			<<numberStepper "최대 모유량" $milk_volume {
				callback: value => {
					V.milk_volume = value;
					V.milk_amount = Math.min(V.milk_amount, V.milk_volume);
					Wikifier.wikifyEval("<<replace '#fluids'>><<cheats-fluids>><</replace>>");
				},
				step: 10,
				valueFormat: value => Math.round(value) + "ml",
				percentage: false, min: 24, max: 3000
			}>>
			<<if $player.penisExist>>
				<<numberStepper "최대 정액량" $semen_volume {
					callback: value => {
						V.semen_volume = value;
						V.semen_amount = Math.min(V.semen_amount, V.semen_volume);
						Wikifier.wikifyEval("<<replace '#fluids'>><<cheats-fluids>><</replace>>");
					},
					step: 10,
					valueFormat: value => Math.round(value) + "ml",
					percentage: false, max: 3000
				}>>
			<</if>>
		</div>
	</div>
	<hr>
	<div class="settingsGrid">
		<div class="settingsHeader options">의상</div>
		<div class="settingsToggleItemWide">
			<<link "모든 특수 의상을 해금한다">>
				<<run for(const c in V.specialClothes) V.specialClothes[c] = "unlocked">>
			<</link>>
			<br>
			<span class="description">내구도가 0 이하로 떨어지면 부서집니다. 그 상태에서 치트 메뉴를 나가버리면 해당 옷은 버려지고, 다시 되돌릴 수 없습니다.</span>
			<br>
		</div>
		<<set _cl_slots to ["upper", "lower", "under_upper", "under_lower", "over_upper", "over_lower", "genitals"]>>
		<<if $worn.face.type.includesAny("covered", "gag", "mask")>>
			<<run _cl_slots.push("face")>>
		<</if>>
		<<set $_clothes to []>>

		<<if $worn.over_upper.integrity gte 1>>
			<div class="settingsToggleItem numberslider-inline">
				<span class="gold"><<clothingicon $worn.over_upper "over_upper">><<worn_over_upper_name_ yi>> 내구도:</span>

				<<set _overupperintegritymax to getSetupClothing('over_upper', $worn.over_upper)?.integrity_max || 200>>
				<<numberslider "$worn.over_upper.integrity" `Math.min($worn.over_upper.integrity, _overupperintegritymax)` 0 _overupperintegritymax 1 {
					onInputChange: value => wikifier("<<updatesidebarimg false>>"),
					value: () => integrityWord(V.worn.over_upper, "over_upper") || `<span class="green">완벽함</span>`
				}>>
			</div>
			<div class="settingsToggleItem numberslider-inline">
				<span class="gold"><<clothingicon $worn.over_upper "over_upper">><<worn_over_upper_name_ yi>> 젖은 정도:</span>
				<<numberslider "$overupperwet" $overupperwet 0 200 1 {
					onInputChange: (value) => cheatsShowWetness('overupperwet', value),
					value: v => wetnessWord('overupper')
				}>>
			</div>
		<<else>>
			<<set $_clothes.push("외투 상의")>>
		<</if>>
		<<if $worn.over_lower.integrity gte 1>>
			<div class="settingsToggleItem numberslider-inline">
				<<set _overlowerintegritymax to getSetupClothing('over_lower', $worn.over_lower)?.integrity_max || 200>>
				<span class="gold"><<clothingicon $worn.over_lower "over_lower">><<worn_over_lower_name_ yi>> 내구도:</span>
				<<numberslider "$worn.over_lower.integrity" `Math.min($worn.over_lower.integrity, _overlowerintegritymax)` 0 _overlowerintegritymax 1 {
					onInputChange: value => wikifier("<<updatesidebarimg false>>"),
					value: () => integrityWord(V.worn.over_lower, "over_lower") || `<span class="green">완벽함</span>`
