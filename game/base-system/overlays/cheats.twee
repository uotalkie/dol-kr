			<<numberStepper "수태" $fame.impreg { callback: value => { V.fame.impreg = value }, max: 2000, reverse: true}>>
			<<numberStepper "노출증" $fame.exhibitionism { callback: value => { V.fame.exhibitionism = value }, max: 2000, reverse: true}>>
			<<if playerNormalPregnancyTotal() lt 8>>
				<span class="description"><<set _pregsNeeded to Math.trunc(8-playerNormalPregnancyTotal())>>
