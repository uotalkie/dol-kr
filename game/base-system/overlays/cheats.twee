			<</if>>
		<<case "positive">>
			<<numberStepper "전투" $fame.scrap { callback: value => { V.fame.scrap = value }, max: 2000}>>
			<<numberStepper "친절" $fame.good { callback: value => { V.fame.good = value }, max: 2000}>>
			<<numberStepper "사업" $fame.business { callback: value => { V.fame.business = value }, max: 2000}>>
			<<numberStepper "사교" $fame.social { callback: value => { V.fame.social = value }, max: 2000}>>
			<<numberStepper "모델" $fame.model { callback: value => { V.fame.model = value }, max: 2000}>>
		<<case "skills">>
			<<numberStepper "속임수" $skulduggery "skulduggery" {max: 1000}>>
			<<numberStepper "춤" $danceskill "danceskill" {max: 1000}>>
			<<numberStepper "수영" $swimmingskill "swimmingskill" {max: 1000}>>
			<<numberStepper "운동" $athletics "athletics" {max: 1000}>>
			<<numberStepper "관리" $tending "tending" {max: 1000}>>
			<<numberStepper "정리정돈" $housekeeping "housekeeping" {max: 1000}>>
