				<</capture>>
			<</for>>
			<<unset _cr>>
		<<case "negative">>
			<<numberStepper "섹스" $fame.sex { callback: value => { V.fame.sex = value }, max: 2000, reverse: true}>>
			<<numberStepper "매춘" $fame.prostitution { callback: value => { V.fame.prostitution = value }, max: 2000, reverse: true}>>
