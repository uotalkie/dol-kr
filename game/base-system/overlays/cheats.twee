			<<numberStepper "<<scienceicon>> 과학" $science "science" {
				callback: (value) => wikifier('adjust_school_traits'),
				max: 1000
			}>>
			<<numberStepper "<<mathicon>> 수학" $maths "maths" {
				callback: (value) => wikifier('adjust_school_traits'),
				max: 1000
			}>>
			<<numberStepper "<<englishicon>> 영어" $english "english" {
				callback: (value) => wikifier('adjust_school_traits'),
				max: 1000
			}>>
			<<numberStepper "<<historyicon>> 역사" $history "history" {
				callback: (value) => wikifier('adjust_school_traits'),
				max: 1000
			}>>
		<<case "characteristics">>
			<<numberStepper "<<tficon angel>> 순결도" $purity "purity" {max: 1000}>>
			<<numberStepper "<<symbol beauty>> 용모" $beauty "beauty" {max: 10000}>>
			<<numberStepper "<<symbol physique>> 신체 능력" $physique "physique" {max: $physiquesize}>>
			<<numberStepper "<<symbol willpower>> 의지력" $willpower "willpower" {max: 1000}>>
			<<numberStepper "<<symbol awareness>> 성지식" $awareness "awareness" {max: 1000, reverse: true}>>
			<<numberStepper "<<symbol promiscuity>> 음란도" $promiscuity "promiscuity" {reverse: true}>>
			<<numberStepper "<<symbol exhibitionism>> 노출증" $exhibitionism "exhibitionism" {reverse: true}>>
			<<numberStepper "<<symbol deviancy>> 이상성욕" $deviancy "deviancy" {reverse: true}>>
			<<if numberOfEarSlime()>>
				<<numberStepper "<<symbol corruption>> 오염도" $earSlime.corruption { callback: value => {V.earSlime.corruption = value; Wikifier.wikifyEval("<<updatesidebarimg>>")}, reverse: true}>>
			<</if>>
		<<case "sex">>
			<<numberStepper "유혹" $seductionskill "seductionskill" {max: 1000}>>
			<<numberStepper "가슴" $chestskill "chestskill" {max: 1000}>>
			<<numberStepper "엉덩이" $bottomskill "bottomskill" {max: 1000}>>
			<<if $player.vaginaExist>><<numberStepper "보지" $vaginalskill "vaginalskill" {max: 1000}>><</if>>
			<<if $player.penisExist>><<numberStepper "자지" $penileskill "penileskill" {max: 1000}>><</if>>
			<<numberStepper "허벅지" $thighskill "thighskill" {max: 1000}>>
			<<numberStepper "입" $oralskill "oralskill" {max: 1000}>>
			<<numberStepper "손" $handskill "handskill" {max: 1000}>>
			<<numberStepper "항문" $analskill "analskill" {max: 1000}>>
			<<numberStepper "발" $feetskill "feetskill" {max: 1000}>>
		<<default>>
			ERROR: Skill issue.
	<</switch>>
<</widget>>
