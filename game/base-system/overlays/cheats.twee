					<<set $vaginastate to 0>>
					<<set $penisuse to "none">>
					<<set $penisstate to "none">>
					<<set $player.gender to "f">>
					<<set $player.sex to "f">>
					<<set $player.ballsExist to false>>
					<<set $player.penisExist to false>>
					<<set $player.vaginaExist to true>>
					<<set $sexStats.vagina.menstruation.running = true>>
					<<updatesidebarimg>>
					<<replace "#statsGender">><<cheatsGender>><</replace>>
				<</link>> |
			<</if>>
			<<if $player.gender is "h">>
				<span class="gold">후타나리</span>
			<<else>>
				<<link "후타나리">>
					<<wash>>
					<<set $vaginause to 0>>
					<<set $vaginastate to 0>>
					<<set $penisuse to 0>>
					<<set $penisstate to 0>>
					<<set $player.gender to "h">>
					<<set $player.sex to "h">>
					<<set $player.penisExist to true>>
					<<set $player.vaginaExist to true>>
					<<set $sexStats.vagina.menstruation.running = true>>
					<<updatesidebarimg>>
					<<replace "#statsGender">><<cheatsGender>><</replace>>
				<</link>>
			<</if>>
		<</if>>
		<<if $player.gender is "h">>
			<br>
			후타나리 고환: <span id="statsBallsExist"><<= $player.ballsExist ? "있음" : "없음">></span> |
			<<link "변경">>
				<<set $player.ballsExist to clone(!$player.ballsExist)>>
				<<updatesidebarimg>>
				<<replace "#statsGender">><<cheatsGender>><</replace>>
			<</link>>
		<</if>>
	<<else>>
		<span class="gold"><<= $player.gender is "m" ? "남자" : $player.gender is "f" ? "여자" : "후타나리">></span> | <<if _reasons.length gte 1>>이 설정은 <<= _reasons.join("고 ")>>기 때문에 변경할 수 없습니다. 일반적으로, 이<<else>>이<</if>> 설정은 당신이 옷장에 있을 때에만 변경할 수 있습니다.
	<</if>>
<</widget>>

<<widget "cheatSection">>
	<<switch _args[0]>>
		<<case "state">>
			<<numberStepper "고통" $pain { callback: value => { V.pain = value; Wikifier.wikifyEval("<<updatesidebarimg>>") }, max: 200, reverse: true}>>
			<<numberStepper "흥분" $arousal { callback: value => { V.arousal = value; Wikifier.wikifyEval("<<updatesidebarimg>>") }, max: 10000, reverse: true}>>
			<<numberStepper "피로" $tiredness { callback: value => { V.tiredness = value; Wikifier.wikifyEval("<<updatesidebarimg>>") }, max: 2000, reverse: true}>>
			<<numberStepper "스트레스" $stress { callback: value => { V.stress = value; Wikifier.wikifyEval("<<updatesidebarimg>>") }, max: 10000, reverse: true}>>
			<<numberStepper "트라우마" $trauma { callback: value => { V.trauma = value; Wikifier.wikifyEval("<<updatesidebarimg>>") }, max: 5000, reverse: true}>>
