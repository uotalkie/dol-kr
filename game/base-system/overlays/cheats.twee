						<<set _hermaphrodite to false>>
					<</link>>
					<<= _i lt _parasiteTypes.length - 1 ? "|" : "">>
				<</for>>
				<br>
				<<if $_pregnancy.motherStatus lt 2>>
					<label><<checkbox "_hermaphrodite" false true autocheck>> 자웅동체</label>
