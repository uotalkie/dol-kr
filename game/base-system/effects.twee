		<<set $body_temperature_outside to "cold">>
	<<elseif $warmth lte $chill>>
		<<set $body_temperature_outside to "chilly">>
	<<else>>
		<<set $body_temperature_outside to "comfy">>
	<</if>>
<</if>>

