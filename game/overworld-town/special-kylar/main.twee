	<<else>>
		<<switch $bus>>
			<<case "park">>
				<<set $_dest to "Park">>
			<<case "schoollibrary">>
				<<set $_dest to "School Library">>
			<<case "starfish">>
				<<set $_dest to "Arcade">>
			<<case "kylarmanor">>
				<<set $_dest to "Manor Kylar Room">>
			<<case "rehearsal">>
